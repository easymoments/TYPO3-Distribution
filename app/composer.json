{
	"repositories": [
	],
	"name": "josefglatz/typo3-distribution",
	"description": "A TYPO3 CMS kickstarter distribution package in composer mode",
	"_comment": [
		"This is my TYPO3 distribution",
		"based on https://github.com/josefglatz/TYPO3-Distribution"
	],
	"authors": [
		{
			"name": "Josef Glatz",
			"email": "j.glatz@supseven.at",
			"role": "Developer",
			"homepage": "https://www.supseven.at"
		},
		{
			"name": "Josef Glatz",
			"email": "jousch@jousch.com",
			"role": "Developer",
			"homepage": "https://jousch.com"
		}
	],
	"license": "GPL-3.0+",
	"support": {
		"issues": "https://github.com/josefglatz/TYPO3-Distribution/issues"
	},
	"minimum-stability": "stable",
	"config": {
		"optimize-autoloader": true,
		"secure-http": true,
		"sort-packages": true,
		"bin-dir": ".",
		"platform": {
			"php": "7.2"
		}
	},
	"require": {
		"php": "^7.2",
		"georgringer/autoswitchtolistview": "dev-master",
		"georgringer/gdpr": "^1.0",
		"georgringer/news": "^7",
		"georgringer/page_speed": "dev-master",
		"helhum/typo3-console": "^5",
		"ichhabrecht/content-defender": "^3",
		"josefglatz/beuser-fastswitch": "^2",
		"josefglatz/iconcheck": "^2",
		"lochmueller/staticfilecache": "^6.1",
		"minifranske/gravatar": "^1",
		"typo3/cms-about": "^9.5",
		"typo3/cms-adminpanel": "^9.5",
		"typo3/cms-belog": "^9.5",
		"typo3/cms-beuser": "^9.5",
		"typo3/cms-context-help": "^9.5",
		"typo3/cms-cshmanual": "^9.5",
		"typo3/cms-filelist": "^9.5",
		"typo3/cms-filemetadata": "^9.5",
		"typo3/cms-fluid-styled-content": "^9.5",
		"typo3/cms-form": "^9.5",
		"typo3/cms-info": "^9.5",
		"typo3/cms-info-pagetsconfig": "^9.5",
		"typo3/cms-linkvalidator": "^9.5",
		"typo3/cms-lowlevel": "^9.5",
		"typo3/cms-opendocs": "^9.5",
		"typo3/cms-recycler": "^9.5",
		"typo3/cms-redirects": "^9.5",
		"typo3/cms-reports": "^9.5",
		"typo3/cms-rte-ckeditor": "^9.5",
		"typo3/cms-saltedpasswords": "^9.5",
		"typo3/cms-scheduler": "^9.5",
		"typo3/cms-setup": "^9.5",
		"typo3/cms-sys-action": "^9.5",
		"typo3/cms-sys-note": "^9.5",
		"typo3/cms-taskcenter": "^9.5",
		"typo3/cms-tstemplate": "^9.5",
		"typo3/cms-viewpage": "^9.5",
		"typo3/cms-wizard-crpages": "^9.5",
		"typo3/cms-wizard-sortpages": "^9.5",
		"typo3/minimal": "^9.5"
	},
	"require-dev": {
		"namelesscoder/typo3-repository-client": "^2.0",
		"neronmoon/scriptsdev": "^0.1.2",
		"roave/security-advisories": "dev-master"
	},
	"suggest": {
	},
	"extra": {
		"typo3/cms": {
			"cms-package-dir": "{$vendor-dir}/typo3/cms",
			"web-dir": "web"
		},
		"helhum/typo3-console": {
			"install-binary": false,
			"install-extension-dummy": false
		},
		"scripts-dev": {
			"post-autoload-dump": [
				"@folder-structure"
			]
		}
	},
	"autoload": {
		"classmap": [
		],
		"psr-4": {
			"JosefGlatz\\Theme\\": "web/typo3conf/ext/theme/Classes",
			"JosefGlatz\\ThemeProject\\": "web/typo3conf/ext/theme/Classes"
		}
	},
	"scripts": {
		"clean-generated-files": [
			"rm -rf ./web/typo3conf/PackageStates.php  || true"
		],
		"package-states": [
			"@php ./typo3cms install:generatepackagestates"
		],
		"folder-structure": [
			"@php ./typo3cms install:fixfolderstructure"
		],
		"setup-active-extensions": [
			"@php ./typo3cms extension:setupactive"
		],
		"toolchain-install": [
			"./sgc-core/install.sh --local && cd ./web/typo3conf/ext/theme/ && npm install"
		],
		"toolchain-update": [
			"cd ./web/typo3conf/ext/theme/ && npm install"
		],
		"toolchain-watch": [
			"./sgc build && ./sgc"
		],
		"toolchain-watch-silent": [
			"./sgc build && ./sgc -s"
		],
		"toolchain-watch-nobrowsersync": [
			"./sgc build && ./sgc watch"
		],
		"toolchain-build": [
			"./sgc build"
		],
		"php-cs-fixer": [
			"php-cs-fixer fix --config ./Build/.php_cs"
		],
		"pre-deploy": [
			"./sgc build"
		],
		"post-autoload-dump": [
			"@package-states"
		],
		"post-install-cmd": [
		],
		"post-update-cmd": [
		]
	}
}
