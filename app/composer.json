{
	"repositories": [
	],
	"name": "josefglatz/typo3-distribution",
	"description": "A TYPO3 CMS kickstarter distribution package in composer mode",
	"_comment": [
		"This is my TYPO3 distribution",
		"based on https://github.com/josefglatz/TYPO3-Distribution"
	],
	"authors": [
		{
			"name": "Josef Glatz",
			"email": "jousch@jousch.com",
			"role": "Developer",
			"homepage": "https://jousch.com"
		}
	],
	"license": "GPL-3.0+",
	"support": {
		"issues": "https://github.com/josefglatz/TYPO3-Distribution/issues"
	},
	"minimum-stability": "stable",
	"config": {
		"optimize-autoloader": true,
		"secure-http": true,
		"sort-packages": true,
		"bin-dir": ".",
		"platform": {
			"php": "7.2"
		}
	},
	"require": {
		"php": "^7.2",
		"georgringer/autoswitchtolistview": "dev-master",
		"georgringer/gdpr": "^1.0",
		"georgringer/news": "^7",
		"georgringer/page_speed": "dev-master",
		"helhum/typo3-console": "^5",
		"ichhabrecht/content-defender": "^3",
		"josefglatz/beuser-fastswitch": "^2",
		"josefglatz/iconcheck": "^2",
		"josefglatz/seo-aspect-ratios": "^1.0",
		"lochmueller/staticfilecache": "^6.1",
		"minifranske/gravatar": "^1",
		"t3monitor/t3monitoring_client": "^9.0",
		"typo3/cms-about": "dev-master as 9.5.1",
		"typo3/cms-adminpanel": "dev-master as 9.5.1",
		"typo3/cms-backend": "dev-master as 9.5.1",
		"typo3/cms-belog": "dev-master as 9.5.1",
		"typo3/cms-beuser": "dev-master as 9.5.1",
		"typo3/cms-core": "dev-master as 9.5.1",
		"typo3/cms-extbase": "dev-master as 9.5.1",
		"typo3/cms-extensionmanager": "dev-master as 9.5.1",
		"typo3/cms-filelist": "dev-master as 9.5.1",
		"typo3/cms-filemetadata": "dev-master as 9.5.1",
		"typo3/cms-fluid": "dev-master as 9.5.1",
		"typo3/cms-fluid-styled-content": "dev-master as 9.5.1",
		"typo3/cms-form": "dev-master as 9.5.1",
		"typo3/cms-frontend": "dev-master as 9.5.1",
		"typo3/cms-info": "dev-master as 9.5.1",
		"typo3/cms-install": "dev-master as 9.5.1",
		"typo3/cms-linkvalidator": "dev-master as 9.5.1",
		"typo3/cms-lowlevel": "dev-master as 9.5.1",
		"typo3/cms-opendocs": "dev-master as 9.5.1",
		"typo3/cms-recordlist": "dev-master as 9.5.1",
		"typo3/cms-recycler": "dev-master as 9.5.1",
		"typo3/cms-redirects": "dev-master as 9.5.1",
		"typo3/cms-reports": "dev-master as 9.5.1",
		"typo3/cms-rte-ckeditor": "dev-master as 9.5.1",
		"typo3/cms-scheduler": "dev-master as 9.5.1",
		"typo3/cms-seo": "dev-master as 9.5.1",
		"typo3/cms-setup": "dev-master as 9.5.1",
		"typo3/cms-sys-action": "dev-master as 9.5.1",
		"typo3/cms-sys-note": "dev-master as 9.5.1",
		"typo3/cms-taskcenter": "dev-master as 9.5.1",
		"typo3/cms-tstemplate": "dev-master as 9.5.1",
		"typo3/cms-viewpage": "dev-master as 9.5.1"
	},
	"require-dev": {
		"friendsofphp/php-cs-fixer": "^2.13",
		"namelesscoder/typo3-repository-client": "^2.0",
		"neronmoon/scriptsdev": "^0.1.4",
		"phpmd/phpmd": "^2.6",
		"psychomieze/adminpanel-extended": "dev-develop",
		"roave/security-advisories": "dev-master",
		"squizlabs/php_codesniffer": "^3.3"
	},
	"suggest": {
	},
	"extra": {
		"typo3/cms": {
			"cms-package-dir": "{$vendor-dir}/typo3/cms",
			"web-dir": "web"
		},
		"helhum/typo3-console": {
			"install-binary": false,
			"install-extension-dummy": false
		},
		"scripts-dev": {
			"post-autoload-dump": [
				"@folder-structure"
			]
		}
	},
	"autoload": {
		"classmap": [
		],
		"psr-4": {
			"JosefGlatz\\Theme\\": "web/typo3conf/ext/theme/Classes",
			"JosefGlatz\\ThemeProject\\": "web/typo3conf/ext/theme/Classes"
		}
	},
	"scripts": {
		"clean-generated-files": [
			"rm -rf ./web/typo3conf/PackageStates.php  || true"
		],
		"package-states": [
			"@php ./typo3cms install:generatepackagestates"
		],
		"folder-structure": [
			"@php ./typo3cms install:fixfolderstructure"
		],
		"setup-active-extensions": [
			"@php ./typo3cms extension:setupactive"
		],
		"toolchain-install": [
			"./sgc-core/install.sh --local && cd ./web/typo3conf/ext/theme/ && npm install"
		],
		"toolchain-update": [
			"cd ./web/typo3conf/ext/theme/ && npm install"
		],
		"toolchain-watch": [
			"./sgc build && ./sgc"
		],
		"toolchain-watch-silent": [
			"./sgc build && ./sgc -s"
		],
		"toolchain-watch-nobrowsersync": [
			"./sgc build && ./sgc watch"
		],
		"toolchain-build": [
			"./sgc build"
		],
		"php-cs-fixer": [
			"php-cs-fixer fix --config ./Build/.php_cs"
		],
		"pre-deploy": [
			"./sgc build"
		],
		"post-autoload-dump": [
			"@package-states"
		],
		"post-install-cmd": [
		],
		"post-update-cmd": [
		]
	}
}
